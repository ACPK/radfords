<table>
  <tr>
    <th>Title</th>

    <th>Description</th>
  </tr>

  <% @products.each do |product| %>
    <tr>
      <td><%= product.title %></td>

      <td><%= product.description %></td>

      <td><%= image_tag(product.image_url) %></td>

      <td><%= product.price %></td>
    </tr>
  <% end %>
</table>

<%= link_to 'New Product', new_product_path %>
